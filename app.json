{
  "expo": {
    "name": "Chef Minute",
    "slug": "chef-minute-react-native-pwa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/main-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-circle.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icons/main-icon.png"
    }
  }
}
